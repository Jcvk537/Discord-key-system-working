local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Orion/main/source"))()

local Window = OrionLib:MakeWindow({
	Name = "Key System - Proton Aim",
	HidePremium = false,
	SaveConfig = false,
	IntroText = "Loading protonaim.dll..."
})

OrionLib:MakeNotification({
	Name = "Proton Notifier",
	Content = "protonaim.dll has been loaded.",
	Image = "rbxassetid://4483345998",
	Time = 3
})

_G.Key = "LION4ZP"
_G.KeyInput = ""

-- Quando la chiave √® corretta, carica lo script principale
function MakeScriptHub()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/USERNAME/REPOSITORY/main/protonaim.lua"))()
end

-- Notifica chiave corretta
function CorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "‚úÖ Correct Key!",
        Content = "Access granted to Proton Aim.",
        Image = "rbxassetid://4483345998",
        Time = 3
    })
end

-- Notifica chiave sbagliata
function IncorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "‚ùå Incorrect Key!",
        Content = "Access denied. Invalid key.",
        Image = "rbxassetid://4483345998",
        Time = 3
    })
end

-- TAB della chiave
local Tab = Window:MakeTab({
	Name = "üîê Key System",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

Tab:AddButton({
	Name = "‚úÖ Check Key",
	Callback = function()
		if _G.KeyInput == _G.Key then
			CorrectKeyNotification()
			wait(1)
			MakeScriptHub()
		else
			IncorrectKeyNotification()
		end
	end
})
